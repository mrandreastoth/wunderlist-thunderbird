<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>nicolas.arnaudcormos@gmail.com</em:id>
    <em:name>Wunderlist Task Button</em:name>
    <em:version>0.1</em:version>
    <em:creator>Nicolas Arnaud-Cormos</em:creator>
    <em:description>Non-official addon for Wunderlist. Easily transform a mail into a task for Wunderlist with a single click.</em:description>
    <em:iconURL>chrome://wunderlist/content/icon.png</em:iconURL>
    <em:homepageURL>http://www.wunderlist.com/</em:homepageURL>

    <!-- Unpack this extension -->
    <em:unpack>true</em:unpack>

    <!-- This is an extension -->
    <em:type>2</em:type>

    <!-- Thunderbird -->
    <em:targetApplication>
      <Description>
        <em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
        <em:minVersion>3.0</em:minVersion>
        <em:maxVersion>22.0</em:maxVersion>
      </Description>
    </em:targetApplication>

  </Description>
</RDF>
